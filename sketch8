background (255, 255, 255);
size (720, 720);

int x=0;
int y=0;

for (x=0+25; x< width; x += 32){
  for (y=0+25; y < height-20; y += 32){
    noStroke ();
    colorMode (HSB, 360, 100, 100);
    fill (x/2.5, y/2.5, 100); //i didnt manage to make it go the right direction
    ellipse (x, y, 30, 30);
  }
}
